{
  "name": "@hungry/package-configuration",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "TBD",
  "sideEffects": false,
  "babel": {
    "presets": [
      "@hungry/cli"
    ]
  },
  "private": false,
  "scripts": {
    "build:code": "babel src/** -d dist --extensions '.ts','.tsx'",
    "prepublish": "yarn test && yarn build",
    "test": "jest",
    "build:declarations": "tsc --build tsconfig.json",
    "build": "concurrently 'yarn build:declarations' 'yarn build:code'",
    "watch": "concurrently 'yarn build:declarations --watch' 'yarn build:code --watch'",
    "docs": "typedoc --out docs src --sourcefile-url-prefix https://github.com/hungry-consulting/package-configuration/tree/master/src/",
    "gh-pages": "rm -rf docs && yarn docs && gh-pages -d docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "testRegex": "/__tests__/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": ">=8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hungry-consulting/package-configuration"
  },
  "dependencies": {
    "cosmiconfig": "^5.0.6",
    "@types/cosmiconfig": "^5.0.3",
    "ramda": "^0.25.0",
    "fp-ts": "^1.10.0",
    "semver": "^5.6.0",
    "utility-types": "^2.1.0"
  },
  "devDependencies": {
    "typedoc": "^0.13.0",
    "typedoc-plugin-sourcefile-url": "^1.0.3",
    "gh-pages": "^2.0.1",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@hungry/babel-preset-cli": "^0.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "concurrently": "^4.0.1",
    "jest": "^23.6.0",
    "typescript": "^3.1.3"
  }
}